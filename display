 private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {
navigation n=new navigation();
n.show();
dispose(); // TODO add your handling code here:
 }
 private void lsubmitActionPerformed(java.awt.event.ActionEvent evt) {
int amount=Integer.parseInt(lamount.getText());
int income=Integer.parseInt(lincome.getText());
int credit=Integer.parseInt(lcredit.getText());
if(amount>=100000&&amount<=200000)
{
 if(income>=10000)
 {
 if(credit>=10&&credit<=100)
 {
 ltext.setText("Eligible for loan");
 }
 else
 {
 ltext.setText("not eligible for loan");
 }
 }
 else
 {
 ltext.setText("Not eligible for loan");
 }
}
else if(amount>200000&&amount<=500000)
{
 if(income>=30000)
 {
 if(credit>=10&&credit<=100)
 {
 ltext.setText("Eligible for loan");
 }
 else
 {
 ltext.setText("Not Eligible for loan");
 }
 }
 else
 {
 ltext.setText("Not Eligible for loan");
 }

}
else if(amount>500000&&amount<=1000000)
 {
 if(income>=50000)
 {
 if(credit>=10&&credit<=100)
 {
 ltext.setText("Eligible for loan");
 }
 ltext.setText("Not eligible for loan");

 }
 else
 {
 ltext.setText("Not eligible for loan");
 }
 }
else
{
 ltext.setText("Sorry sir/mam we don't give that much amount");
}
// TODO add your handling code here:
 }
