 private void asubmitActionPerformed(java.awt.event.ActionEvent evt) {
 String a=aname.getText();
int acc_no=Integer.parseInt (aaccno.getText());
int pno=Integer.parseInt(aphone.getText());
int age=Integer.parseInt(aage.getText());
int pan=Integer.parseInt(apan.getText());
int pay =Integer.parseInt(apay.getText());
 if(age>18)
 {
 if(pay>=200)
 {
 atext.setText("your ATM card has been applied successfully");
 }
 else
 {
 atext.setText("please pay your payment");
 }
 }
 else
 {
 atext.setText("Not Eligible to apply ATM card");
 }
// TODO add your handling code here:
 }
