 private void wsubmitActionPerformed(java.awt.event.ActionEvent evt) {
int x = 0;
String a=wpin.getText();
int b=Integer.parseInt(wamount.getText());
int balance=1000;
if(balance>=b)
{
 balance=balance-b;
 w.setText("your amount is successfully withdraw and balance amount
is"+balance);
}
else
{
 w.setText("Insufficent balance");
}
